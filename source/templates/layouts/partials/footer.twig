<footer class="footer">
    <div class="footer-socialLinks">
        {% include "../../partials/social-links.twig" %}
    </div>
    <nav class="footer-mobileNav">
        <ul class="mobileNav-list">
            <li class="mobileNav-item">
                <a href="about.html" class="mobileNav-link">О компании</a>
            </li>
            <li class="mobileNav-item">
                <a href="traiding.html" class="mobileNav-link">Торговля</a>
            </li>
            <li class="mobileNav-item">
                <a href="tools.html" class="mobileNav-link">Инструменты</a>
            </li>
            <li class="mobileNav-item">
                <a href="parthers.html" class="mobileNav-link">Партнерство</a>
            </li>
        </ul>
    </nav>
    <div class="footer-inner">
        <div class="footer-row footer-row--nav">
            {% for column in footerNav %}
                <div class="footer-column footer-column--nav">
                    <nav class="footer-nav">
                        <ul class="footer-nav-list">
                            {% for navigation in column %}
                                <li class="footer-nav-item">
                                    {% if navigation.title and navigation.slug %}
                                        <a class="footer-nav-link" href="{{ navigation.slug }}">{{ navigation.title }}</a>
                                    {% endif %}
                                    {% if navigation.submenu %}
                                    <ul class="footer-subnav">
                                        {% for submenu in navigation.submenu %}
                                            <li class="footer-subnav-item">
                                                <a class="footer-subnav-link" href="{{ submenu.slug }}">{{ submenu.title }}</a>
                                            </li>
                                        {% endfor %}
                                    </ul>
                                    {% endif %}
                                </li>
                            {% endfor %}
                        </ul>
                    </nav>
                </div>
            {% endfor %}
        </div>
        <div class="footer-row">
            <div class="footer-certificates">
                <ul class="certificates-list">
                    <li class="certificates-item">
                        <a class="certificates-item-link" href="">
                            <img src="images/certificates/sysec.png" alt="sysec">
                        </a>
                    </li>
                    <li class="certificates-item">
                        <a class="certificates-item-link" href="">
